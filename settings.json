{
  "editor.fontFamily": "Fira Code, Fira Mono, Consolas",
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "go.gopath": "%USERPROFILE%/go",
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.smartcase": true,
  "vim.timeout": 250,
  "terminal.integrated.fontSize": 14,
  "vim.leader": "<space>",
  "editor.cursorBlinking": "solid",
  "window.zoomLevel": 0,
  "git.confirmSync": false,
  "files.associations": {
    "*.vue": "vue",
    "*.jinja": "jinja",
    "*.t": "perl"
  },
  "go.formatTool": "goimports",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.external.osxExec": "iTerm2.app",
  "workbench.activityBar.visible": false,
  "go.docsTool": "godoc",
  "workbench.colorTheme": "Night Owl",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "boundary",
    "editor.quickSuggestions": false,
    "editor.acceptSuggestionOnEnter": "smart"
  },
  "[lua]": {
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 3,
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": false,
  "python.pythonPath": "python3",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "vim.sneak": true,
  "vim.easymotionMarkerFontFamily": "Fira Mono",
  "vim.easymotionMarkerFontSize": "14",
  "vim.easymotionMarkerYOffset": 12,
  "vim.easymotionMarkerHeight": 24,
  "vim.easymotionMarkerBackgroundColor": "rgba(40, 40, 40, 0.5)",
  "vim.insertModeKeyBindings": [
    {
      "before": ["<Ctrl>"],
      "after": ["w"]
    }
  ],
  "editor.formatOnSave": true,
  "workbench.startupEditor": "newUntitledFile",
  "go.useLanguageServer": false,
  "workbench.panel.defaultLocation": "right",
  "extensions.ignoreRecommendations": false,
  "terminal.integrated.macOptionIsMeta": true,
  "prettier.semi": false,
  "eslint.packageManager": "yarn",
  "workbench.editor.showTabs": true,
  "editor.fontLigatures": true,
  "git.ignoreMissingGitWarning": true,
  "terminal.integrated.shell.windows": "wsl.exe",
  "window.smoothScrollingWorkaround": true,
  "go.autocompleteUnimportedPackages": true,
  "workbench.statusBar.feedback.visible": false,
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#011627",
    "statusBar.noFolderBackground": "#011627",
    "statusBar.debuggingBackground": "#011627"
  },
  "vim.statusBarColors.insert": "#ffe34d",
  "vim.statusBarColors.normal": "#011627",
  "vim.statusBarColors.visual": "#011627",
  "vim.statusBarColors.visualblock": "#011627",
  "vim.statusBarColors.visualline": "#011627"
}
